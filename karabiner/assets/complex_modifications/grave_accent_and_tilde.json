{
  "title": "Right Ctrl + ESC -> grave_accent_and_tilde",
  "maintainers": ["Ying"],
  "rules": [
    {
      "description": "Right Ctrl + ESC -> grave_accent_and_tilde",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde"
            }
          ]
        }
      ]
    }
  ]
}
